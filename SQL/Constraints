--Create Primary key for D_Play_list_items table
ALTER TABLE D_PLAY_LIST_ITEMS
ADD CONSTRAINT D_PLM_PK
PRIMARY KEY (EVENT_ID, SONG_ID);

--Create Primary key for D_songs table
ALTER TABLE D_SONGS
ADD CONSTRAINT D_SONGS_PK 
PRIMARY KEY(ID);

--Create Primary key for D_track_listings table
ALTER TABLE D_TRACK_LISTINGS
ADD CONSTRAINT D_TLG_PK
PRIMARY KEY(SONG_ID, CD_NUMBER);

--Create Primary key for D_types table
ALTER TABLE D_TYPES
ADD CONSTRAINT D_TYPES_PK
PRIMARY KEY (CODE);

--Create Primary key for D_clients table
ALTER TABLE D_CLIENTS
ADD CONSTRAINT D_CLIENTS_PK
PRIMARY KEY (CLIENT_NUMBER);

--Create Primary key for D_events table
ALTER TABLE D_EVENTS
ADD CONSTRAINT D_EVENTS_PK
PRIMARY KEY (ID);

--Create Primary key for D_job_assignments table
ALTER TABLE D_JOB_ASSIGNMENTS
ADD CONSTRAINT D_JOB_ASSIGNMENTS_PK
PRIMARY KEY (PARTNER_ID, EVENT_ID);

--Create Primary key for D_partners table
ALTER TABLE D_PARTNERS
ADD CONSTRAINT D_PARTNERS_PK
PRIMARY KEY (ID);

--Create Primary key for D_venues table
ALTER TABLE D_VENUES
ADD CONSTRAINT D_VENUES_PK
PRIMARY KEY (ID);

--Create Primary key for D_themes table
ALTER TABLE D_THEMES
ADD CONSTRAINT D_THEMES_PK
PRIMARY KEY (CODE);

--Create Primary key for D_Packages table
ALTER TABLE D_PACKAGES
ADD CONSTRAINT D_PACKAGES_PK
PRIMARY KEY (CODE);

--Create Primary key for D_cds table
ALTER TABLE D_CDS
ADD CONSTRAINT D_CDS_PK
PRIMARY KEY (CD_NUMBER);

--Create foreign key for d_play_list_items
ALTER TABLE D_PLAY_LIST_ITEMS
ADD CONSTRAINT D_PLM_EVENT_ID_FK 
FOREIGN KEY (EVENT_ID) REFERENCES D_EVENTS (ID);

ALTER TABLE D_PLAY_LIST_ITEMS
ADD CONSTRAINT D_PLM_SONG_ID_FK 
FOREIGN KEY (SONG_ID) REFERENCES D_SONGS (ID);

--Create foreign keys for D-events
ALTER TABLE D_EVENTS 
ADD CONSTRAINT D_EVE_CLIENT_NUMBER_FK
FOREIGN KEY (CLIENT_NUMBER) REFERENCES D_CLIENTS (CLIENT_NUMBER);

ALTER TABLE D_EVENTS
ADD CONSTRAINT D_EVE_PACKAGE_CODE_FK 
FOREIGN KEY (PACKAGE_CODE) REFERENCES D_PACKAGES (CODE);

ALTER TABLE D_EVENTS
ADD CONSTRAINT D_EVE_THEME_CODE_FK 
FOREIGN KEY (THEME_CODE) REFERENCES D_THEMES (CODE);

ALTER TABLE D_EVENTS
ADD CONSTRAINT D_EVE_VENUE_ID_FK 
FOREIGN KEY (VENUE_ID) REFERENCES D_VENUES (ID);

--create foreign Keys for d_job_assignments
ALTER TABLE D_JOB_ASSIGNMENTS
ADD CONSTRAINT D_JAT_EVENT_ID_FK 
FOREIGN KEY (EVENT_ID) REFERENCES D_EVENTS (ID);

ALTER TABLE D_JOB_ASSIGNMENTS
ADD CONSTRAINT D_JAT_PARTNER_ID_FK 
FOREIGN KEY (PARTNER_ID) REFERENCES D_PARTNERS (ID);

--create foreign Key for D-Songs
ALTER TABLE D_SONGS
ADD CONSTRAINT D_SNG_TYPE_CODE_FK 
FOREIGN KEY (TYPE_CODE) REFERENCES D_TYPES (CODE);

--create foreign key for d_track_listings
ALTER TABLE D_TRACK_LISTINGS
ADD CONSTRAINT  D_TLG_CD_NUMBER_FK 
FOREIGN KEY (CD_NUMBER) REFERENCES D_PARTNERS (ID);

ALTER TABLE D_TRACK_LISTINGS
ADD CONSTRAINT D_TLG_SONG_ID_FK 
FOREIGN KEY (SONG_ID) REFERENCES D_SONGS (ID);



